  <!--submit.js-->
  
   function presssubmit(){
  var submit = document.getElementById('submit');
  var q1 = document.getElementById("Q1");

  if (q1.style.display === "none"){
    q1.style.display = "block";
  } else{
    submit.style.display = "none";
  }
}
